def valid_url(url): pattern = r"^https?://([a-zA-Z0-9-.]+|\d{1,3}(.\d{1,3}){3})(:\d+)?(/.*)?$" return re.match(pattern, url) is not None

def extract_host(url): return urlparse(url).netloc.split(":")[0]

def port_open(ip, port): try: socket.create_connection((ip, port), timeout=2).close() return True except: return False

def get_cf_cookie(target): if not uc: print(f"{RED}[x] Modul undetected-chromedriver tidak tersedia!{RESET}") exit() print(f"{YELLOW}[â€¢] Meluncurkan Stealth Chrome...{RESET}") options = uc.ChromeOptions() options.add_argument("--headless=new") options.add_argument("--no-sandbox") options.add_argument("--disable-blink-features=AutomationControlled") try: driver = uc.Chrome(options=options) driver.get(target) time.sleep(6) cookies = driver.get_cookies() cf_clearance = next((c['value'] for c in cookies if c['name'] == 'cf_clearance'), None) ua = driver.execute_script("return navigator.userAgent") driver.quit() if not cf_clearance: print(f"{RED}[x] Gagal mendapatkan cf_clearance cookie.{RESET}") exit() return cf_clearance, ua except Exception as e: print(f"{RED}[x] Stealth mode error: {e}{RESET}") exit()

